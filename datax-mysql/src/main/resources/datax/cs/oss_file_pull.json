{
  "core": {
    "container": {
      "job": {
        "sleepInterval": 200
      }
    }
  },
  "job": {
    "setting": {
      "speed": {
        "channel": 4
      },
      "errorLimit": {
        "record": 0
      }
    },
    "content": [
      {
        "reader": {
          "name": "mysqlreader",
          "parameter": {
            "username": "${mainUsername}",
            "password": "${mainPassword}",
            "connection": [
              {
                "jdbcUrl": [
                  "${mainUrl}"
                ],
                "querySql": [
                  "select * from oss_file where id in (${fileIds})"
                ]
              }
            ]
          }
        },
        "writer": {
          "name": "mysqlwriter",
          "parameter": {
            "username": "${username}",
            "password": "${password}",
            "writeMode": "replace",
            "column": [
              "*"
            ],
            "connection": [
              {
                "table": [
                  "oss_file"
                ],
                "jdbcUrl": "${url}"
              }
            ]
          }
        }
      }
    ]
  }
}

